{
  "name": "@naturalcycles/json2html",
  "scripts": {
    "json2html": "tsn ./src/bin/json2html.ts",
    "json2html-debug": "tsn ./src/bin/json2html.ts './src/test/**/*.json' --debug"
  },
  "dependencies": {
    "@naturalcycles/js-lib": "^4.8.1",
    "@naturalcycles/promise-lib": "^2.2.0",
    "fs-extra": "^7.0.1",
    "globby": "^9.2.0",
    "loud-rejection": "^2.1.0",
    "type-fest": "^0.4.1",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@naturalcycles/dev-lib": "^7.3.1",
    "@naturalcycles/semantic-release": "^1.0.0",
    "@types/node": "^12.0.0",
    "jest": "^24.7.1"
  },
  "files": [
    "dist",
    "src",
    "!src/test",
    "!src/**/*.test.ts",
    "!src/**/__snapshots__",
    "!src/**/__exclude"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "json2html": "dist/bin/json2html.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NaturalCycles/json2html"
  },
  "engines": {
    "node": ">=10.13"
  },
  "version": "1.0.0",
  "description": "Render arbitrary json as human-friendly html. CLI and API.",
  "keywords": [
    "json",
    "html",
    "json2html",
    "json-to-html",
    "cli"
  ],
  "author": "Natural Cycles Team",
  "license": "MIT"
}
